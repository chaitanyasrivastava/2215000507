<!DOCTYPE html>
<html>
<head>
    <title>Latest Posts</title>
</head>
<body>
    <h1>Latest Posts</h1>
    <ul>
        <% posts.forEach(post => { %>
            <li>ID: <%= post.id %>, Content: <%= post.content %>, Timestamp: <%= post.timestamp.toLocaleTimeString() %> - <%= post.timestamp.toLocaleDateString() %></li>
        <% }); %>
    </ul>
    <a href="/">Back to Home</a>
</body>
</html>